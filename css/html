    js
    <script>
        function updateIndice() {
          var indiceBotao = document.getElementById("indice-botao-button");
          var titulos = document.querySelectorAll(".Post-especial-text h2");
          var scrollTop = window.scrollY || window.pageYOffset;

          for (var i = 0; i < titulos.length; i++) {
            var titulo = titulos[i];
            var tituloOffset = titulo.offsetTop;

            if (tituloOffset - 180 <= scrollTop) { // Alterado para considerar 5px acima do h2
              indiceBotao.textContent = titulo.textContent;
            }
          }
        }

        function toggleIndice() {
          var titulos = document.querySelectorAll(".Post-especial-text h2");
          var indiceBotao = document.getElementById("indice-botao-button");

          if (titulos.length > 0) {
            indiceBotao.textContent = titulos[0].textContent;
          }
        }

        window.addEventListener("scroll", updateIndice);
        toggleIndice(); // Chamada inicial para definir o texto do botão
    </script>